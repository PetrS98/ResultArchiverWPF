<UserControl x:Class="WPFUtilsLib.UserControls.IOs.OnlineStatusDot"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFUtilsLib.UserControls.IOs"
             Width="20">
    <Grid>
        <Image
            x:Name="PART_greenDot" 
            Source="/WPFUtilsLib;component/Resources/GreenDot.png"/>
        <Image
            x:Name="PART_whiteDot" 
            Source="/WPFUtilsLib;component/Resources/WhiteDot.png"/>
        <Image
            x:Name="PART_redDot" 
            Source="/WPFUtilsLib;component/Resources/RedDot.png"/>
    </Grid>
</UserControl>
